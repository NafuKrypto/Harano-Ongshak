<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    >
<item>
    <shape android:shape="rectangle"  >
    <corners
        android:radius="14dp"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="@color/colorBrand3"
        android:startColor="@color/colorBrand3"
        android:endColor="@color/colorBrand3"
        android:type="linear"

        />
    <padding
        android:left="2dp"
        android:top="15dp"
        android:right="2dp"
        android:bottom="5dp"
        />
    <size
        android:width="100dp"
        android:height="100dp"
        />
    <stroke
        android:width="1dp"
        android:color="@color/colorBrand3"
        />

    </shape>
</item>

    <item
        android:drawable="@drawable/add"
        android:width="50dp"
        android:height="50dp"
        android:gravity="center_horizontal"
        android:layout_marginBottom="5dp"
        />

</layer-list>